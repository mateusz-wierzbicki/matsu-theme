  "name": "matsu-theme",
  "homepage": "https://matsu-theme.vercel.app",
  "items": [
    {
      "name": "matsu-theme",
      "type": "registry:style",
      "description": "Ghibli Studio style inspired theme for shadcn/ui with watercolor paper texture and unique color palette",
