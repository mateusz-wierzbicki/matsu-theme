{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "matsu-theme",
  "homepage": "https://matsu-theme.vercel.app",
  "items": [
    {
      "name": "login",
      "type": "registry:component",
      "title": "Login",
      "description": "A login page",
      "registryDependencies": ["avatar", "button", "input", "label"],
      "files": [
        {
          "path": "app/login/page.tsx",
          "type": "registry:component",
          "target": "app"
        },
        {
          "path": "components/login-form.tsx",
          "type": "registry:component"
        }
      ]
    },
    {
      "name": "matsu-theme-components",
      "type": "registry:component",
      "title": "Matsu Theme Components",
      "description": "A collection of components for the Matsu Theme",
      "files": [
        {
          "path": "components/ui/button.tsx",
          "type": "registry:component",
          "target": "components/ui"
        }
      ]
    }
  ]
}
